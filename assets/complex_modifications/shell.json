{
  "title": "execute some script",
  "rules": [
    {
      "description": "execute some script",
      "manipulators": [
        {
          "description": "Open Terminal",
          "type": "basic",
          "from": {
            "key_code": "y",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_control",
                "left_option",
                "left_shift"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a terminal.app"
            }
          ]
        }
      ]
    }
  ]
}