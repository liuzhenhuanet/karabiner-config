          "description": "Open Terminal",
          "type": "basic",
          "from": {
            "key_code": "y",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_control",
                "left_option",
                "left_shift"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "shell_command": "sh /Users/liuzhenhua/Workspace/tools/open-applications/open_terminal"
            }
